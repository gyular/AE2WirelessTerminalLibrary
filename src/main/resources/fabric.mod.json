{
  "schemaVersion": 1,
  "id": "ae2wtlib",
  "version": "${version}",
  "name": "AE2WTLib",
  "description": "Adds Wireless versions of ae2 Terminals with infinite range capabilities.",
  "authors": ["mari_023", "Ridanisaurus", "DomamaN202"],
  "contact": {
    "homepage": "https://modrinth.com/mod/applied-energistics-2-wireless-terminals",
    "sources": "https://github.com/Mari023/AE2WirelessTerminalLibrary",
    "issues": "https://github.com/Mari023/AE2WirelessTerminalLibrary/issues"
  },
  "license": "MIT",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "ae2:client": ["de.mari_023.ae2wtlib.client.AE2wtlibClient"],
    "ae2": ["de.mari_023.ae2wtlib.AE2wtlibEntrypoint"],
    "rei_client": [
      "de.mari_023.ae2wtlib.rei.Plugin"
    ],
    "modmenu": ["de.mari_023.ae2wtlib.ModMenuIntegration"]
  },
  "mixins": [
    "ae2wtlib.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "minecraft": "*",
    "ae2": ">=${ae2_version}",
    "cloth-config2": "*"
  },
  "suggests": {
    "trinkets": "*"
  }
}
